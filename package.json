{
  "name": "gulp-htmlincluder",
  "version": "1.5.14",
  "description": "Gulp plugin for building files into each other.  Made for a workflow involving responsive unit testing.",
  "keywords": [
    "gulpplugin"
  ],
  "homepage": "https://github.com/internetErik/gulp-htmlincluder",
  "bugs": "https://github.com/internetErik/gulp-htmlincluder/issues",
  "author": {
    "name": "Erik Christianson",
    "url": "https://github.com/internetErik"
  },
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/internetErik/gulp-htmlincluder.git"
  },
  "scripts": {
    "build": "webpack -p",
    "webpack": "webpack -w"
  },
  "dependencies": {
    "gulp-util": "^3.0.8",
    "natives": "^1.1.6",
    "through2": "*"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              ">0.25%",
              "not ie 11",
              "not op_mini all"
            ]
          }
        }
      ],
      "es2015",
      "stage-2"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "coveralls": "*",
    "event-stream": "*",
    "gulp": "^3.9.1",
    "istanbul": "*",
    "mocha": "*",
    "mocha-lcov-reporter": "*",
    "should": "~2.1.0",
    "webpack": "^4.23.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-s3-plugin": "^1.0.2"
  },
  "licenses": [
    {
      "type": "MIT"
    }
  ]
